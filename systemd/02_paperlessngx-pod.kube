[Install]
WantedBy=default.target

[Unit]
Wants=podman-user-wait-network-online.service
After=podman-user-wait-network-online.service

[Service]
ExecStartPre=/bin/sleep 10

[Kube]
# Point to the yaml file in the same directory
Yaml=/srv/apps/02_paperlessngx/02_paperlessngx-pod.yaml
Network=homeserver01-network
# Use  config map in the same directory
ConfigMap=/srv/apps/02_paperlessngx/02_paperlessngx-configmap.yaml